{
  "name": "expressmakers/expressmakers-php",
  "description": "PHP Library for ExpressMakers API (with laravel support)",
  "homepage": "https://developers.expressmakers.com",
  "type": "library",
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "ExpressMakers\\API\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ExpressMakers\\API\\ExpressMakersServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Pezhvak",
      "email": "pezhvakimv@gmail.com",
      "homepage": "https://pezhvak.imvx.org"
    }
  ],
  "minimum-stability": "dev"
}